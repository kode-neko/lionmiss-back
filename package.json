{
  "name": "lionmiss-back",
  "version": "0.0.0",
  "description": "LionMiss back",
  "type": "module",
  "main": "index.js",
  "author": "Kode Neko",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"npm run start:rest\" \"npm run start:gql\"",
    "start:rest": "node ./dist/rest",
    "start:gql": "node ./dist/graphql",
    "dev:rest": "npm run nodemon ./dist/rest",
    "dev:gql": "npm run nodemon ./dist/graphql",
    "build": "tsc --project tsconfig.json",
    "build:dev": "tsc --watch --project tsconfig.json",
    "nodemon": "nodemon ./dist/index.js",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "dependencies": {
    "apollo-server": "^3.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "lionmiss-core": "^0.3.4",
    "lodash": "^4.17.21",
    "mongodb": "^5.2.0",
    "mongoose": "^7.0.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "xss-purge": "^1.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "chalk": "^5.2.0",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "figlet": "^1.5.2",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^9.1.5",
    "jest": "^29.5.0",
    "jest-transform-stub": "^2.0.0",
    "mongodb-memory-server": "^8.12.2",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "yargs": "^17.7.1"
  }
